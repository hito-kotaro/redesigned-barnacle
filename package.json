{
  "name": "drcmquest",
  "version": "0.1.0",
  "description": "",
  "main": "server.js",
  "engines": {
    "node": "16.13.x",
    "npm": "8.1.0"
  },
  "scripts": {
    "up": "node_modules/.bin/sequelize db:migrate --env development",
    "down": "node_modules/.bin/sequelize db:migrate:undo --env development",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "ts-node src/server.ts",
    "dev:watch": "ts-node-dev --respawn src/server.ts",
    "clean": "rimraf dist/*",
    "tsc": "tsc",
    "build": "npm-run-all clean tsc",
    "start": "node server.js",
    "check-types": "tsc --noEmit",
    "eslint:fix": "eslint src/**/*.ts --fix",
    "eslint": "eslint src/**/*.ts",
    "format": "prettier --write 'src/**/*.{js,ts,json}'",
    "lint": "npm-run-all eslint check-types format",
    "lint:fix": "npm-run-all eslint check-types format"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-jwt": "^6.1.2",
    "express-jwt-authz": "^2.4.1",
    "express-oauth2-jwt-bearer": "^1.1.0",
    "jwks-rsa": "^2.1.4",
    "mysql2": "^2.3.3",
    "nodemon": "^2.0.19",
    "sequelize": "^6.21.4",
    "sequelize-cli": "^6.4.1"
  },
  "devDependencies": {
    "@types/node": "^16.11.48",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "8.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.7.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  }
}
